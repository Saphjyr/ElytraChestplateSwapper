{
  "schemaVersion": 1,
  "id": "ecs",
  "version": "${version}",

  "name": "Elytra/Chestplate Swapper",
  "description": "Simple quality of life mod that swaps between chestplate and elytra on a keystroke",
  "authors": [
    "Saphjyr"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/Saphjyr/ElytraChestplateSwapper"
  },

  "license": "CC0-1.0",
  "icon": "assets/ecs/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
    ],
    "client":[
      "com.saphjyr.ElytraChestplateSwapper.ElytraChestplateSwapperClient"
    ]
  },
  "mixins": [
    "ecs.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": "1.20.x",
    "fabric-key-binding-api-v1": "*",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
